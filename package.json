{
  "name": "simekiri-bot",
  "version": "0.0.1",
  "description": "HX simekiri bot",
  "main": "index.js",
  "scripts": {
    "start": "node $npm_package_main",
    "build": "npm run build:ts && npm run build:babel",
    "build:babel": "babel es6 --out-dir lib",
    "build:ts": "tsc -p .",
    "watch:babel": "babel --watch es6 --out-dir lib",
    "watch:ts": "tsc -w -p .",
    "watch:build": "npm run clean && npm run build && concurrently 'npm run watch:ts' 'npm run watch:babel'",
    "clean": "rimraf lib es6"
  },
  "author": "Shumpei Shiraishi",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "bluebird": "^3.3.4",
    "botkit": "0.0.12",
    "command-line-args": "^2.1.6",
    "concurrently": "^2.0.0",
    "continuation-local-storage": "^3.1.6",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "lodash": "^4.7.0",
    "minimist": "^1.2.0",
    "moment": "^2.12.0",
    "mysql": "^2.10.2",
    "node-cron": "^1.1.1",
    "rimraf": "^2.5.2",
    "sequelize": "^3.20.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "tslint": "^3.6.0",
    "typescript": "^1.8.9",
    "typings": "^0.7.9",
    "watch": "^0.17.1"
  }
}
